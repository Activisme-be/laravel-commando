{
  "name": "vkovic/laravel-commando",
  "description": "Collection of handy Laravel artisan commands that most projects needs",
  "keywords": [
    "laravel",
    "console",
    "artisan",
    "commands"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Vladimir KoviÄ‡",
      "email": "vlada.kovic@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Vkovic\\LaravelCommando\\": "src"
    },
    "files": [
        "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Vkovic\\LaravelCommando\\Test\\": "tests"
    }
  },
  "require": {
    "php": "^7.1",
    "laravel/framework": "5.5.*|5.6.*|5.7.*|5.8.*"
  },
  "require-dev": {
    "orchestra/testbench": "3.5.*|3.6.*|3.7.*|3.8.*",
    "orchestra/database": "3.5.*|3.6.*|3.7.*|3.8.*",
    "phpunit/phpunit": "^6.3|^7.0"
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
      "laravel": {
          "providers": [
              "Vkovic\\LaravelCommando\\Providers\\LaravelCommandoServiceProvider"
          ]
      }
  },
  "minimum-stability": "dev"
}